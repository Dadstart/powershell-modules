{
    "powershell.powerShellDefaultVersion": "PowerShell 7.4",
    "powershell.integratedConsole.showOnStartup": true,
    "powershell.integratedConsole.focusConsoleOnExecute": false,
    "powershell.codeFormatting.enable": true,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.preset": "Custom",
    "powershell.codeFormatting.openBraceOnSameLine": true,
    "powershell.codeFormatting.newLineAfterOpenBrace": true,
    "powershell.codeFormatting.whitespaceBeforeOpenBrace": true,
    "powershell.codeFormatting.whitespaceBeforeOpenParen": true,
    "powershell.codeFormatting.whitespaceAroundOperator": true,
    "powershell.codeFormatting.whitespaceAfterSeparator": true,
    "powershell.codeFormatting.alignPropertyValuePairs": true,
    "powershell.codeFormatting.newLineAfterCloseBrace": true,
    "powershell.codeFormatting.whitespaceInsideBrace": true,
    "powershell.codeFormatting.whitespaceAroundPipe": true,
    "powershell.codeFormatting.addWhitespaceAroundPipe": true,
    "powershell.codeFormatting.ignoreOpenBraceOnSameLine": false,
    "powershell.codeFormatting.ignoreOpenParenOnSameLine": false,
    "powershell.codeFormatting.ignoreMaxLineLength": false,
    "powershell.codeFormatting.ignoreOneLineBlock": false,
    "powershell.codeFormatting.ignoreHashComments": false,
    "powershell.codeFormatting.ignoreComments": false,
    "powershell.codeFormatting.ignoreStrings": false,
    "powershell.codeFormatting.ignoreHereStrings": false,
    "powershell.codeFormatting.ignorePipes": false,
    "powershell.codeFormatting.ignoreAssignment": false,
    "powershell.codeFormatting.ignoreType": false,
    "powershell.codeFormatting.ignoreSemicolons": false,
    "powershell.codeFormatting.ignoreTrailingWhitespace": false,
    "powershell.codeFormatting.ignoreUnusedExpression": false,
    "powershell.codeFormatting.ignoreRedundantWhitespace": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceSemicolon": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundPipe": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundSeparator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundOpenBrace": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundCloseBrace": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundOpenParen": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundCloseParen": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundOpenBracket": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundCloseBracket": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundComma": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundDot": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundColon": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundSemicolon": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundEquals": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundPlus": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundMinus": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundMultiply": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundDivide": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundModulo": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundPower": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundAnd": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundOr": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundXor": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNot": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundShiftLeft": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundShiftRight": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundAssignment": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundAssignmentOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundComparisonOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundLogicalOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundBitwiseOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundUnaryOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundBinaryOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundTernaryOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullCoalescingOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalOperator": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalMemberAccess": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalElementAccess": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalInvocation": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalIndex": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalProperty": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalMethod": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalField": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalEvent": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalDelegate": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalEnum": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalStruct": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalInterface": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalClass": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalNamespace": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalUsing": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalExtern": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalStatic": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalReadonly": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalConst": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalVolatile": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalUnsafe": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalFixed": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalStackalloc": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalCheck": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalUncheck": false,
    "powershell.codeFormatting.ignoreRedundantWhitespaceAroundNullConditionalDefault": false,

    "powershell.scriptAnalysis.enable": true,
    "powershell.scriptAnalysis.settingsPath": "PSScriptAnalyzerSettings.psd1",
    "powershell.scriptAnalysis.updateBuildProblemMatcher": "automatic",

    "markdownlint.config": {
        "MD022": false,
        "MD031": false,
        "MD032": false,
        "MD013": {
            "line_length": 160,
            "code_blocks": true,
            "code_block_line_length": 160,
            "tables": false,
            "headings": false,
        }
    },

    "files.associations": {
        "*.ps1": "powershell",
        "*.psd1": "powershell",
        "*.psm1": "powershell",
        "*.ps1xml": "xml"
    },
    "search.exclude": {
        "**/BuildOutput": true,
        "**/Release": true,
        "**/Logs": true,
        "**/node_modules": true,
        "**/bin": true,
        "**/obj": true
    },
    "files.exclude": {
        "**/BuildOutput": true,
        "**/Release": true,
        "**/Logs": true,
        "**/node_modules": true,
        "**/bin": true,
        "**/obj": true
    }
} 